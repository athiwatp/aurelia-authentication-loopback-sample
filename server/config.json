{
  "restApiRoot": "/api",
  "host": "localhost",
  "port": 3000,
  "client": "http://localhost:9000",
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": {
      "origin": true,
      "credentials": true
    },
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "jwt": {
    "user_model": "user",
    "client_secret": "**do_this_in_config_local_json**",
    "access_token_ttl": "1d",
    "refresh_token_ttl": "30d",
    "properties": {
      "displayName": "displayName",
      "email": "email",
      "userId": "id"
    }
  }
}
